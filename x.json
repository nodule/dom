      "phrases": {
        "active": "Finding closest parent: {{input.selector}}"
      },
      "dependencies": {
        "npm": {
          "discore-closest": "0.2.x"
        }
      },
      "expose": [
        "document"
      ],
      "description": " Finds the closest parent that matches a selector.",
      "async": true,
      "title": "Closest",
      "ns": "dom",
      "name": "closest",
      "fn": "on.input.element = function(data) {\n  var sel = discore_closest(\n    data, input.selector, input.checkSelf,\n    input.within || document\n  );\n  var res = { element: data };\n  if (sel) {\n    res.selection = sel;\n  } else {\n    res.error = new Error('Selector didn\\'t match:' + input.selector);\n  }\n\n  output(res);\n\n};\n"
    },
