{
  "name": "querySelector",
  "ns": "dom",
  "title": "querySelector",
  "description": "[Document query selector](https://developer.mozilla.org/en-US/docs/Web/API/document.querySelector)",
  "expose": ["window","document"],
  "phrases": {
    "active": "Gathering elements matching criteria: {{input.selector}}"
  },
  "ports": {
    "input": {
      "selector": {
        "title": "Selector",
        "type": "string"
      },
      "document": {
        "title": "Dom Document",
        "type": "function",
        "default": null
      }
    },
    "output": {
      "selection": {
        "type": "object"
      }
    }
  }
}
