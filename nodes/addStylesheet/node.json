{
  "name": "addStylesheet",
  "ns": "dom",
  "async": true,
  "description": "Adds a stylesheet to the head of the HTML page",
  "phrases": {
    "active": "Attaching script {input.url}"
  },
  "ports": {
    "input": {
      "url": {
        "type": "string",
        "async": true,
        "format": "url",
        "title": "Script Url"
      }
    },
    "output": {
      "head": {
        "type": "HTMLElement",
        "title": "Head"
      },
      "link": {
        "type": "HTMLElement",
        "title": "Link"
      }
    }
  }
}