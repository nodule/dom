{
  "name": "domify",
  "ns": "dom",
  "title": "Domify",
  "description": "Turn HTML into DOM elements x-browser",
  "phrases": {
    "active": "Domifying HTML"
  },
  "ports": {
    "input": {
      "in": {
        "title": "Html",
        "type": "string",
        "format": "html",
        "required": "true"
      }
    },
    "output": {
      "out": {
        "title": "Dom Elements",
        "type": [
          "HTMLElement",
          "DocumentFragment"
        ]
      }
    }
  },
  "dependencies": {
    "npm": {
      "domify": "0.x.x"
    }
  }
}