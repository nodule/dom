{
  "name": "domify",
  "ns": "dom",
  "title": "Domify",
  "description": "Turn HTML into DOM elements x-browser",
  "require": {
    "domify": "0.x.x"
  },
  "phrases": {
    "active": "Domifying HTML"
  },
  "ports": {
    "input": {
      "in": {
        "title": "Html",
        "type": "string",
        "format": "html",
        "required": "true"
      }
    },
    "output": {
      "out": {
        "title": "Dom Elements",
        "type": ["HTMLElement","DocumentFragment"]
      }
    }
  }
}
