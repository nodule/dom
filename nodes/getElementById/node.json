{
  "name": "getElementById",
  "ns": "dom",
  "title": "getElementById",
  "async": true,
  "description": "[Get element by ID](https://developer.mozilla.org/en-US/docs/Web/API/document.getElementById)",
  "expose": ["document"],
  "phrases": {
    "active": "Getting element with id: {{input.id}}"
  },
  "ports": {
    "input": {
      "id": {
        "title": "ID",
        "type": "string",
        "async": true,
        "required": "true"
      }
    },
    "output": {
      "element": {
        "type": "object"
      }
    }
  }
}
