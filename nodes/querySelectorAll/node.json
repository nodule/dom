{
  "name": "querySelectorAll",
  "ns": "dom",
  "title": "querySelectorAll",
  "expose": [
    "window",
    "document"
  ],
  "description": "[Document querySelectorAll](https://developer.mozilla.org/en-US/docs/Web/API/document.querySelectorAll)",
  "phrases": {
    "active": "Gathering all elements matching criteria: {{input.selector}}"
  },
  "ports": {
    "input": {
      "element": {
        "title": "Element",
        "type": "HTMLElement",
        "default": null
      },
      "selector": {
        "title": "Selector",
        "type": "string",
        "required": true
      }
    },
    "output": {
      "element": {
        "title": "Element",
        "type": "HTMLElement",
        "default": null
      },
      "selection": {
        "type": "object"
      },
      "error": {
        "type": "Error",
        "title": "Error"
      }
    }
  }
}