{
  "name": "setValue",
  "ns": "dom",
  "description": "Set value of an input element",
  "phrases": {
    "active": "Setting value for {input.element.name}"
  },
  "ports": {
    "input": {
      "in": {
        "type": "any",
        "title": "Value"
      },
      "element": {
        "type": "HTMLElement",
        "title": "Dom Element",
        "async": true
      }
    },
    "output": {
      "element": {
        "type": "HTMLElement",
        "title": "Dom Element"
      },
      "out": {
        "type": "string",
        "title": "Value"
      }
    }
  }
}
