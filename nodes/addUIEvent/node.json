{
  "name": "addUIEvent",
  "ns": "dom",
  "description": "Add UI Event Listener",
  "phrases": {
    "active": "Adding {{input.event}} handler"
  },
  "ports": {
    "input": {
      "element": {
        "type": "HTMLElement",
        "title": "Dom Element"
      },
      "event": {
        "type": "string",
        "enum": [
          "focusin",
          "focusout",
          "DOMActivate"
        ],
        "title": "Dom Event"
      }
    },
    "output": {
      "element": {
        "type": "HTMLElement",
        "title": "Dom Element"
      },
      "event": {
        "type": "object",
        "title": "Event"
      }
    }
  }
}