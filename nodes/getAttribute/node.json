  "ns": "dom",
  "title": "Get Attribute",
  "async": true,
  "description": "Get attribute",
  "expose": ["document"],
  "phrases": {
    "active": "Getting attribute: {{input.attribute}}"
  },
  "ports": {
    "input": {
      "element": {
        "title": "Element",
